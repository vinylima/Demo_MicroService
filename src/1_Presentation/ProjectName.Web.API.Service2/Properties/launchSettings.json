{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:4000",
      "sslPort": 0
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express - Dev": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/Environment",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "IIS Express - Prod": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/Environment",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      }
    },
    "Kestrel - Dev": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/Environment",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:4000"
    },
    "Kestrel - Prod": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/Environment",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Production"
      },
      "applicationUrl": "http://localhost:4000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/Environment",
      "publishAllPorts": true
    }
  }
}